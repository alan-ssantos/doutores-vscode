{
  "name": "doutores",
  "displayName": "doutores-vscode",
  "description": "Kit de ferramentas",
  "author": {
    "name": "Alan Santos"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alan-ssantos/doutores-vscode.git"
  },
  "homepage": "https://github.com/alan-ssantos/doutores-vscode/blob/main/README.md",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "doutores.abrirMpis",
        "title": "Abrir todas Mpis"
      },
      {
        "command": "doutores.textToUrl",
        "title": "Texto para URL"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.71.0",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "remove-accents": "^0.4.3"
  }
}